CREATE TEMPORARY TABLE tmp_sku (
    sku VARCHAR(128) NOT NULL,
    entity_id INTEGER NOT NULL,
    type_id VARCHAR(32) NOT NULL,
    PRIMARY KEY (sku),
    UNIQUE INDEX (entity_id)
)
SELECT
    product.sku,
    product.entity_id,
    product.type_id
FROM catalog_product_entity AS product
WHERE product.sku IS NOT NULL;